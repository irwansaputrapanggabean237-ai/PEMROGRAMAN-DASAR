        "def hitung_faktorial(n):\n",
        "    \"\"\"Menghitung faktorial dari bilangan positif n dengan penanganan kasus khusus.\"\"\"\n",
        "\n",
        "    if n < 0:\n",
        "        return \"Error: Faktorial tidak terdefinisi untuk bilangan negatif.\"\n",
        "    elif n == 0:\n",
        "        return 1\n",
        "\n",
        "    hasil = 1\n",
        "\n",
        "    while n > 1:\n",
        "        hasil *= n\n",
        "        n -= 1  # Corrected from T to 1\n",
        "    return hasil\n",
        "\n",
        "bilangan_n = 4 # Defined the variable\n",
        "print (f\"Faktorial dari {bilangan_n} adalah: {hitung_faktorial (bilangan_n)}\") # Corrected function name\n",
        "print(f\"Faktorial dari 0 adalah: {hitung_faktorial (0)}\") # Corrected function name\n",
        "print (f\"Faktorial dari -3 adalah: {hitung_faktorial(-3)}\")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
